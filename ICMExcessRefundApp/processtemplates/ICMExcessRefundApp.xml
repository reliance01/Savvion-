    <PreFunction type="javascript:function">/* type your javascript function body here */

var proposalid = jst.getDataSlot("ProposalID");



var sb = new java.lang.StringBuffer();

sb.append("&lt;html&gt;&lt;style type=\"text/css\" &gt;&lt;!--.mailbody {font-family: Calibri, Helvetica,sans-serif;font-size:14px}--&gt;&lt;!--.tabletitle {width:200px;font-weight:bold;color:#000080;}--&gt;&lt;!--.tablecell {width:200px;}--&gt;&lt;!--.tablecls {width:800px; cellpadding:15px}--&gt;&lt;/style&gt;&lt;body class=\"mailbody\"&gt;&lt;br/&gt;");

sb.append("Dear Approver, ");

sb.append("&lt;br/&gt;&lt;br/&gt;Proposal " + proposalid + " is pending for excess refund approval in Savvion, kindly taking necessary action");

sb.append("&lt;/table&gt;");

sb.append("&lt;br/&gt;&lt;br/&gt;Request you to do the needful. Click &lt;b&gt;&lt;a href=\"http://bpm.reliancegeneral.co.in/sbm/bpmportal/login.jsp\" target=\"_blank\"&gt;here&lt;/a&gt; &lt;/b&gt;on this link to login into the system");				

sb.append("&lt;br/&gt;&lt;br/&gt;Thank you,");

sb.append("&lt;br/&gt;Reliance Application Support Team");

sb.append("&lt;br/&gt;&lt;br/&gt;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp; ************* THIS IS SYSTEM GENERATED MAIL. PLEASE DO NOT REPLY. **************&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;");

sb.append("&lt;br/&gt;&lt;/body&gt;&lt;/html&gt;");



var userMailID = jst.getDataSlot("Approver");

var mailFrom = "rgicl.applnsupport@reliancegeneral.co.in";

var hm = new java.util.HashMap();

hm.put("TO", userMailID);

hm.put("FROM", mailFrom);

hm.put("SUBJECT", "Proposal " + proposalid + " is pending for excess refund approval in Savvion");

hm.put("BODY", sb);

hm.put("WORKSTEP_NAME", jst.getWorkstepName());

hm.put("WORKITEM_NAME", jst.getWorkstepName());

hm.put("PTID",jst.getProcessInstanceID());

hm.put("PIID",jst.getProcessInstanceID());

var ems = jst.loadClass("com.icm.EmailSender");

var obj = ems.newInstance();

obj.send(hm);</PreFunction>
    <PostFunction type="javascript:function">var blsession = blserver.connect("rgicl", "rgicl");

var piId = jst.getProcessInstanceID();

var wsName = jst.getWorkstepName();

var wsi = blserver.getWorkStepInstance(blsession, piId, wsName);

var lst = wsi.getCompletedWorkItemPerformer();

jst.putDataSlot("Approver", lst.get(lst.size()-1));</PostFunction>
